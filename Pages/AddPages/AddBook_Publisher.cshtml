@page
@model Unilib.Pages.AddPages.AddBook_PublisherModel
@using Unilib.DatabaseUtil

<h1>Add Book_Publisher</h1>

<form method="post">
    <div class="form-group">
        <label for="ISBN">ISBN:</label>
        <input type="text" class="form-control" id="ISBN" name="ISBN">
    </div>
    <div class="form-group">
        <label for="publisher_id">Publisher id:</label>
        <textarea class="form-control" id="publisher_id" name="Publisher_id"></textarea>
    </div>
    <button type="submit" class="btn btn-primary add-btn">Add</button>
    <a href="/MainPages/Book_Publisher" class="btn btn-danger">Cancel</a>
</form>
